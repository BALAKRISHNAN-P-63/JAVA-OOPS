class TableBlock {
    void printTable(int n) {
        // synchronized block
        synchronized (this) {
            for (int i = 1; i <= 5; i++) {
                System.out.println(n * i);
                try {
                    Thread.sleep(400); // slows down output to see thread behavior
                } catch (Exception e) {
                    System.out.println(e);
                }
            }
        }
    }
}

class MyThread3 extends Thread {
    TableBlock t;
    MyThread3(TableBlock t) { this.t = t; }
    public void run() { t.printTable(5); }
}

class MyThread4 extends Thread {
    TableBlock t;
    MyThread4(TableBlock t) { this.t = t; }
    public void run() { t.printTable(100); }
}

public class SyncBlockDemo {
    public static void main(String[] args) {
        TableBlock obj = new TableBlock();
        MyThread3 t1 = new MyThread3(obj);
        MyThread4 t2 = new MyThread4(obj);
        t1.start();
        t2.start();
    }
}

#OUTPUT
5
10
15
20
25
100
200
300
400
500
